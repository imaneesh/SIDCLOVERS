q = 1
x = 0  
b = "" 
a = ["ABC", "BCD", "CDE", "DEF", "ZAB", "YZA", "XYZ", "JXY"]
c = a[x]
for i in a:
	while x < len(a):
		for y in range(len(a)):
			if a[x][q:q+1] in a[y][0:1]:
				if a[y] in c:                                
					c = a[x][0] + c
                                else:
                                        c = c + a[y][2]	
                x = x + 1
        q = q + 1
print c

